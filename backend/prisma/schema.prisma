generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  email String @unique
  firstName String @map("first_name")
  lastName String @map("last_name")
  password String 

  @@map("user")
}
